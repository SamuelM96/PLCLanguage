/* 
Problem 1 - Prefix
Take a language L and produce the language La = { ax | x âˆˆ L}, that is, the language that is like
L, but an additional a at the start of each word.
*/

function prefix (language, outputNumber){
	temp = {};
	for (word : language){
		temp.add("a" + word);
		if(#temp >= outputNumber){
			break;
		}
	}
	return temp.sort();
}